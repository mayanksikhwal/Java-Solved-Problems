//Program to print the Array in reverse order

import java.lang.*; // Importing necessary Java libraries
import java.util.*;  // Importing utility classes, including Scanner

public class Main {
    public static void main(String[] args) {
        // Creating a Scanner object to take input from the user
        Scanner scn = new Scanner(System.in);

        // Taking an integer input N which represents the size of the array
        int N = scn.nextInt();

        // Declaring an integer array of size N
        int[] Array = new int[N];

        // Loop to take N inputs from the user and store them in the array
        for (int i = 0; i < N; i++) {
            Array[i] = scn.nextInt();  // Storing each element input into the array
        }

        // Loop to print the elements of the array in reverse order
        for (int i = N - 1; i >= 0; i--) {
            System.out.print(Array[i] + " ");  // Printing each element followed by a space
        }
    }
}
